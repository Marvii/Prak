
import java.util.Arrays;
import java.util.List;

public class Main {

    public static void main(String[] args) {
        Integer[] a = new Integer[100];
        for (int i = 0; i < 100; i++){
            a[i] = i+1;
        }
        List<String> z = IterTools.mapAllToList(Arrays.asList(a), (Integer p) -> {return p.toString()+"h";} );
        for (String f:z){
            System.out.println(f);
        }
    }
}
